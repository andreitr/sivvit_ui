<?xml version="1.0"?>
<project basedir="../" default="compile">

	<taskdef name="jscomp" classname="com.google.javascript.jscomp.ant.CompileTask"
	classpath="/Users/andreitaraschuk/Documents/code/closure-compiler/compiler.jar"/>


	<target name="compile">
		<jscomp compilationLevel="simple" warning="verbose"	debug="false" output="compiled/file.js">

			<externs dir="${basedir}/js/libs/">
				<file name="jquery/jquery-1.7.1.min.js" />
			</externs>


			<sources dir="${basedir}/js">
				<file name="compiled_test.js" />
			</sources>

		</jscomp>
	</target>
</project>